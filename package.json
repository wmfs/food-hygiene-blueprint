{
  "name": "@wmfs/food-hygiene-blueprint",
  "version": "0.0.0-semantically-released",
  "description": "Tymly blueprint to support Food Standards Agency open data",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/food-hygiene-blueprint#readme",
  "keywords": [
    "tymly",
    "blueprint",
    "models",
    "state machines",
    "low code"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/food-hygiene-blueprint.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/food-hygiene-blueprint/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "debug": "3.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "standard": "11.0.1",
    "semantic-release": "^15.5.2",
    "@wmfs/tymly": "1.7.0",
    "@wmfs/tymly-etl-plugin": "1.7.0",
    "@wmfs/tymly-pg-plugin": "1.9.0",
    "@wmfs/tymly-test-helpers": "1.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"}
      ]
    }
  }
}
