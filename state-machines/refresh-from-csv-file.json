{
  "Comment": "Blueprint to import food and hygiene ratings data from a CSV file into the model.",
  "version": "1.0",
  "StartAt": "ImportingCsvFiles",
  "instigators": [
    "user"
  ],
  "States": {
    "ImportingCsvFiles": {
      "Type": "Task",
      "InputPath": "$.sourceDir",
      "Resource": "module:importingCsvFiles",
      "ResourceConfig": {
        "modelId": "foodRatings",
        "topDownTableOrder": [
          "food_ratings"
        ],
        "truncateTables": true
      },
      "End": true
    }
  }
}
